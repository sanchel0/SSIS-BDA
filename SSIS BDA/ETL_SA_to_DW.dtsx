        <SQLTask:SqlTaskData
          SQLTask:Connection="{02831C73-0934-4493-9E2A-84133081757A}"
          SQLTask:SqlStatementSource="DECLARE @LastDate DATE = (SELECT MAX(FullDate) FROM dbo.DimDate);&#xA;DECLARE @CurrentDate DATE = GETDATE();&#xA;DECLARE @EndDate DATE = DATEADD(YEAR, 2, @CurrentDate);&#xA;&#xA;-- Si la tabla está vacía, empezar desde '2000-01-01'&#xA;IF @LastDate IS NULL &#xA;    SET @LastDate = '2005-02-15';&#xA;&#xA;-- Solo insertar fechas si la última fecha es menor a la actual&#xA;IF @LastDate &lt; @CurrentDate&#xA;BEGIN&#xA;    DECLARE @NextDate DATE = DATEADD(DAY, 1, @LastDate);&#xA;&#xA;    WHILE @NextDate &lt;= @EndDate&#xA;    BEGIN&#xA;        INSERT INTO dbo.DimDate (&#xA;            DateKey, FullDate, Year, Quarter, MonthNumber, MonthName,&#xA;            WeekOfYear, DayOfMonth, DayOfWeek, DayName&#xA;        )&#xA;        VALUES (&#xA;            CONVERT(INT, FORMAT(@NextDate, 'yyyyMMdd')),  -- DateKey&#xA;            @NextDate,                                    -- FullDate&#xA;            YEAR(@NextDate),                              -- Year&#xA;            DATEPART(QUARTER, @NextDate),                 -- Quarter&#xA;            MONTH(@NextDate),                             -- MonthNumber&#xA;            DATENAME(MONTH, @NextDate),                   -- MonthName&#xA;            DATEPART(WEEK, @NextDate),                    -- WeekOfYear&#xA;            DAY(@NextDate),                               -- DayOfMonth&#xA;            DATEPART(WEEKDAY, @NextDate),                 -- DayOfWeek&#xA;            DATENAME(WEEKDAY, @NextDate)                  -- DayName&#xA;        );&#xA;&#xA;        SET @NextDate = DATEADD(DAY, 1, @NextDate);&#xA;    END;&#xA;END;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
