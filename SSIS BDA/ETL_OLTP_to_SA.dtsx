                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    f.film_id AS FilmId, 
    a.first_name AS FirstName, 
    a.last_name AS LastName, 
    f.title AS Title, 
    f.description AS Description, 
    f.release_year AS ReleaseYear, 
    l.name AS LanguageName, 
    f.rental_duration AS RentalDuration, 
    f.rental_rate AS RentalRate, 
    f.length AS Length, 
    f.replacement_cost AS ReplacementCost, 
    f.rating AS Rating, 
    f.special_features AS SpecialFeatures, 
    f.last_update AS LastUpdate, 
    c.name AS CategoryName,
i.store_id AS StoreId
FROM 
    dbo.film f
INNER JOIN 
    dbo.film_actor fa ON f.film_id = fa.film_id
INNER JOIN 
    dbo.actor a ON fa.actor_id = a.actor_id
INNER JOIN 
    dbo.film_category fc ON f.film_id = fc.film_id
INNER JOIN 
    dbo.category c ON fc.category_id = c.category_id 
INNER JOIN 
    dbo.language l ON f.language_id = l.language_id
INNER JOIN 
    dbo.inventory i ON f.film_id = i.film_id;</property>
